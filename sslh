RUN=yes                                                                         
                                                                                
DAEMON=/usr/sbin/sslh                                                           
                                                                                
DAEMON_OPTS='--user sslh --listen 0.0.0.0:443 --ssh 127.0.0.1:44 --ssl 127.0.0.1:3126 --http 127.0.0.1:80 --openvpn 127.0.0.1:1194 --pidfile /var/run/sslh/sslh.pid'
